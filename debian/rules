# Uncomment this to turn on verbose mode.
# export DH_VERBOSE=1

%:
	dh $@

override_dh_fixperms:
	dh_fixperms
	find debian/ala-i18n/opt/atlas/i18n -type f -exec chmod 644 {} \;
